var start = clock();

class A
{
	var t: int32 = 1;
	var t2: int32 = 2;

	let t3: int32 = 3;

	func toString(): string {

		return typeof(this) + ": " + t + " " + t2 + " " + t3;
	}
}

class B: A
{
	init() {

		this.t = 2;
	}
}

var a = A();

print a.t;

print a.t2;

print a.t3;

a.t2 = 4;

print a.t2;

print a;

var b = B();

print b;

print "" + (clock() - start) + " ms elapsed";
