let x: float = 123;

class Vector2
{
	var x: float = 0;
	var y: float = 0;

	init(X: float, Y: float)
	{
		this.x = X;
		this.y = Y;
	}

	func add(a: Vector2)
	{
		return Vector2(this.x + a.x, this.y + a.y);
	}

	func toString()
	{
		return "(x: " + this.x + ", y: " + this.y + ")";
	}
}

