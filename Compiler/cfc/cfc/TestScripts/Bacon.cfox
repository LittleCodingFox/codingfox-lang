class Food
{
	let name: string;

	init(name: string)
	{
		this.name = name;

		print "Food Init";
	}

	func toString(): string
	{
		return this.name + " (" + typeof(this) + ")";
	}
}

class Bacon: Food
{
	init(name: string)
	{
		print "Bacon init";

		super.init(name);
	}
}

let b: Food = Bacon("My bacon");

print b.toString();

print typeof(b);
